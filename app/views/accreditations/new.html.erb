<h1>New Accreditation</h1>
<%= form_for @accreditation, html: { multipart: true } do |f| %>
  <%= f.hidden_field :user_id %>
  <%= f.hidden_field :start_date %>
  <%= f.hidden_field :end_date %>
  <br/>
  <%= f.submit "Save", class: "btn btn-primary" %>
<% end %>

<br/>
<%= link_to 'Back', accreditations_path %>